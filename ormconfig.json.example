{
   "type": "<DB Type>",
   "host": "<DB Host>",
   "port": 5432,
   "username": "<PostgreSQL DB Username>",
   "password": "<PostgreSQL DB Password>",
   "database": "<PostgreSQL DB Name>",
   "synchronize": true,
   "logging": false,
   "entities": [
      "./dist/src/database/models/**/*.js"
