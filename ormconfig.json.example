      "./dist/src/database/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "./dist/src/database/models",
      "migrationsDir": "./dist/src/database/migration",
      "subscribersDir": "./dist/src/database/subscriber"
   }
}